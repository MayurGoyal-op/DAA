#include <iostream>
using namespace std;
int binarysearch(int arr[], int target, int start, int end)
{
    while (start <= end)
    {
        int mid = start + (end - start) / 2;
        if (arr[mid] == target)
        {
            return mid;
        }
        if (arr[mid < target])
        {
            start = mid + 1;
        }
        else
        {
            end = mid - 1;
        }
    }
    return -1;// not found!
}
int main()
{
    int arr[]={2,5,8,12,16,23,38,56,72,91};
   
    int target;
    cout << "enter target to search :";
    cin>>target;
    int c=binarysearch(arr,target,0,5);
    cout<<c;
    return 0;
}
